<!DOCTYPE html>
<html>
<head>
  <title>SPCA Volunteer Portal</title>
  <%= stylesheet_link_tag    "application", :media => "all" %>
  <%= javascript_include_tag "application" %>
  <%= csrf_meta_tags %>
</head>
<body class="main_css">
<div class="rangaWrapper">
<%= image_tag "SPCA.jpg", class: "logo" %>
  <!--<img src="images/SPCA.jpg" class="logo" />-->
  <div class="rangaContent">
    <%= image_tag "banner.jpg" %>
    <!--<img src="images/banner.jpg" />-->
    <div class="railsWrapper">
      <div id="user_nav">
	<% if current_user %>
	  Logged in as <%= current_user.email %>.
	  <%= link_to "Log out", log_out_path %>
	<% else %>
	  <%= link_to "Sign up", sign_up_path %> or
	  <%= link_to "log in", log_in_path %>
	<% end %>
      </div>
      <br />
      <%= render "layouts/navigation" %>
    
      <%= yield %>

    </div>
  </div>
</div>
</body>
</html>
